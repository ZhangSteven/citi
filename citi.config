# The configuration file for citi.
#

#
# config for the logging section
#
[logging]
log_file=citi.log

# Log level: debug, info, warning, error, critical
log_level=debug

# The directory to store log file, leave it empty if in current directory. 
# Do not put "\" or "/" at the end.
directory=



#
# Input
#
[input]

# The directory to store log file, leave it empty if in current directory. 
directory=



#
# config for the xlrd settings
#
[excel]

# Excel datemode for windows is 0, for mac is 1
datemode = 0		


